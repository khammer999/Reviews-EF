
@model IEnumerable<ReviewSite.Models.Category>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h1> Category Home Page</h1>
    @foreach (var category in Model)
    {
        <div>

            <a href="/Category/Details/@category.Id">Click to view details for 
                @category.Name 
            </a>


        </div>
    }




</body>
</html>
